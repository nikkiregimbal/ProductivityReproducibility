@article{baileyEffectDensityTemperature1988,
  title = {The {{Effect}} of {{Density}} and {{Temperature}} on the {{Swimming}} and {{Aggregating Behaviour}} of the {{Backswimmer}}, {{Anisops}} Deanei ({{Heteroptera}}: {{Notonectidae}}) and {{Subsequent Encounter Rate}} with a {{Sit-and-Wait Predator}}},
  shorttitle = {The {{Effect}} of {{Density}} and {{Temperature}} on the {{Swimming}} and {{Aggregating Behaviour}} of the {{Backswimmer}}, {{Anisops}} Deanei ({{Heteroptera}}},
  author = {Bailey, Paul C. E.},
