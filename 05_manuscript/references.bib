@article{salaGlobalBiodiversityScenarios2000,
  title = {Global {{Biodiversity Scenarios}} for the {{Year}} 2100},
  author = {Sala, Osvaldo E. and Stuart Chapin, F. and III and Armesto, Juan J. and Berlow, Eric and Bloomfield, Janine and Dirzo, Rodolfo and {Huber-Sanwald}, Elisabeth and Huenneke, Laura F. and Jackson, Robert B. and Kinzig, Ann and Leemans, Rik and Lodge, David M. and Mooney, Harold A. and Oesterheld, Mart{\'{\i}}n and Poff, N. LeRoy and Sykes, Martin T. and Walker, Brian H. and Walker, Marilyn and Wall, Diana H.},
