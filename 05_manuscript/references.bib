@article{arazyFrameworkObserverBasedBiases2021,
  title = {A {{Framework}} of {{Observer-Based Biases}} in {{Citizen Science Biodiversity Monitoring}}: {{Semi-Structuring Unstructured Biodiversity Monitoring Protocols}}},
  shorttitle = {A {{Framework}} of {{Observer-Based Biases}} in {{Citizen Science Biodiversity Monitoring}}},
