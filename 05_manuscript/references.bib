}

@article{sanchez-fernandezThermalNicheEstimators2016,
  title = {Thermal Niche Estimators and the Capability of Poor Dispersal Species to Cope with Climate Change},
  author = {{S{\'a}nchez-Fern{\'a}ndez}, David and Rizzo, Valeria and Cieslak, Alexandra and Faille, Arnaud and Fresneda, Javier and Ribera, Ignacio},
  year = {2016},
