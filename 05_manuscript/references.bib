@article{angertFunctionalTradeoffsDetermine2009,
  title = {Functional Tradeoffs Determine Species Coexistence via the Storage Effect},
  author = {Angert, Amy L. and Huxman, Travis E. and Chesson, Peter and Venable, D. Lawrence},
  year = {2009},
