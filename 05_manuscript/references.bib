  author = {Poirotte, Cl{\'e}mence and Massol, Fran{\c c}ois and Herbert, Ana{\"i}s and Willaume, Eric and Bomo, Pacelle M. and Kappeler, Peter M. and Charpentier, Marie J. E.},
