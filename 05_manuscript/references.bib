@article{sadeghiEvaluationSusceptibilityPea2009,
  title = {Evaluation of the {{Susceptibility}} of the {{Pea Aphid}}, {{{\emph{Acyrthosiphon}}}}{\emph{ Pisum}} , to a {{Selection}} of {{Novel Biorational Insecticides}} Using an {{Artificial Diet}}},
