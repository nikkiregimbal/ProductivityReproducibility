@article{leesProductionApterousAlate1967,
  title = {The Production of the Apterous and Alate Forms in the Aphid {{{\emph{Megoura}}}}{\emph{ Viciae}} {{Buckton}}, with Special Reference to the R{\^o}le of Crowding},
  author = {Lees, A. D.},
  year = {1967},
  month = feb,
  journal = {Journal of Insect Physiology},
  volume = {13},
  number = {2},
  pages = {289--318},
  issn = {0022-1910},
  doi = {10.1016/0022-1910(67)90155-2},
  urldate = {2024-10-23},
  abstract = {Although high temperature suppresses the production of alate forms in Megoura, the most significant controlling factor is the degree of crowding. This has been studied by confining aphids in tubes before returning them to the host plant for progeny testing. Adult apterae crowded together in groups of ten for 24 hr produce successive batches of progeny containing alatae and apterae in varying proportions, the number of alatae tending to fall as parturition proceeds. Control aphids isolated in tubes for the same length of time produce only apterous daughters. Crowding effects are therefore due to interactions between the aphids themselves and are not caused by starvation. The response is maternal and begins to appear in the fourth instar. Younger larvae are insensitive. The development of the crowding response is related to the frequency of the encounters between aphids. This was shown by varying either the population density or the size of the container. However, two individuals are adequate to constitute a `crowd', provided the volume of the `arena' is not too great. The contacts must extend over several hours. Overcrowding tends to reduce the response intensity. The sensory mechanisms of the crowding response have been examined. Visual and olfactory stimulation, as well as the exchange of pheromones, can be excuded. Since contacts are required, the relevant sense is probably that of touch. The mechanoreceptors are doubtless the thick-walled articulated bristles on the legs, antennae, and body. Antennal amputation, which removes all the known chemosensilla, fails to abolish the response. Artificial tactile stimulation has so far failed to elicit alata-production. But a variety of alien insects, such as small cockroaches or the beetle Ptinus tectus, are efficient `aphid substitutes'. Insects that are too sluggish to provide many encounters are ineffective. Wing dimorphism is apparently controlled by a diffusible `alata-determiner' liberated from the head of the parent. This agent seems partially to inhibit the secretory activity of the corpus allatum during the late prenatal and early postnatal periods of larval development. But the determination process, which decides the level of secretion, occupies only a few hours prior to birth.},
  file = {C:\Users\nikki\Zotero\storage\JKKECQG3\0022191067901552.html}
}

@article{lefflerMultipleInstancesAncient2013,
