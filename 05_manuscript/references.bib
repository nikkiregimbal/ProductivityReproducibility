@article{molnarMetabolicApproachesUnderstanding2013,
  title = {Metabolic Approaches to Understanding Climate Change Impacts on Seasonal Host-Macroparasite Dynamics},
  author = {Moln{\'a}r, P{\'e}ter K. and Kutz, Susan J. and Hoar, Bryanne M. and Dobson, Andrew P.},
  editor = {Bonsall, Michael},
  year = {2013},
