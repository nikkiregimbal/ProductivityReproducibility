@article{steynDispersalPropensityNot2016,
  title = {Dispersal Propensity, but Not Flight Performance, Explains Variation in Dispersal Ability},
  author = {Steyn, Vernon M. and Mitchell, Katherine A. and Terblanche, John S.},
  year = {2016},
