  file = {C\:\\Users\\nikki\\Zotero\\storage\\DGXLLBNP\\Massot et al. - 2008 - Climate warming, dispersal inhibition and extincti.pdf;C\:\\Users\\nikki\\Zotero\\storage\\2ZM4BGGJ\\j.1365-2486.2007.01514.html}
}

@article{matassaEffectsPredationRisk2015,
  title = {Effects of Predation Risk across a Latitudinal Temperature Gradient},
  author = {Matassa, Catherine M. and Trussell, Geoffrey C.},
