@article{schmidtEvidenceBasedAmphibianConservation2020,
  title = {Evidence-{{Based Amphibian Conservation}}: {{A Case Study}} on {{Toad Tunnels}}},
  shorttitle = {Evidence-{{Based Amphibian Conservation}}},
  author = {Schmidt, Benedikt R. and Brenneisen, Stephan and Zumbach, Silvia},
  year = {2020},
