@article{medrano-vizcainoLandscapeRoadFeatures2023,
  title = {Landscape and Road Features Linked to Wildlife Mortality in the {{Amazon}}},
  author = {{Medrano-Vizca{\'i}no}, Pablo and Grilo, Clara and {Brito-Zapata}, David and {Gonz{\'a}lez-Su{\'a}rez}, Manuela},
