  title = {Larval {{Development}} and {{Molting}}},
  booktitle = {Edible {{Insects}}},
  author = {Kaleka, Amritpal Singh and Kaur, Navkiran and Bali, Gaganpreet Kour and Kaleka, Amritpal Singh and Kaur, Navkiran and Bali, Gaganpreet Kour},
  year = {2019},
