@article{leonInfluencePredatoryBackswimmer1998,
  title = {Influence of the Predatory Backswimmer, {{Notonecta}} Maculata, on Invertebrate Community Structure},
  author = {Leon, Blaustein},
  year = {1998},
  journal = {Ecological Entomology},
  volume = {23},
