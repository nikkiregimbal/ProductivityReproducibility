@article{ellisonPresentFutureHemlock2018,
  title = {The {{Past}}, {{Present}}, and {{Future}} of the {{Hemlock Woolly Adelgid}} ({{{\emph{Adelges}}}}{\emph{ Tsugae}}) and {{Its Ecological Interactions}} with {{Eastern Hemlock}} ({{{\emph{Tsuga}}}}{\emph{ Canadensis}}) {{Forests}}},
