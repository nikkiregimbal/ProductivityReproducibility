@article{borerAsymmetryCommunityRegulation2006,
  title = {Asymmetry in {{Community Regulation}}: {{Effects}} of {{Predators}} and {{Productivity}}},
  shorttitle = {Asymmetry in {{Community Regulation}}},
  author = {Borer, Elizabeth T. and Halpern, Benjamin S. and Seabloom, Eric W.},
  year = {2006},
  journal = {Ecology},
  volume = {87},
