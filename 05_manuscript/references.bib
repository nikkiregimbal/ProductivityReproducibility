@article{dietzDistanceRoadAssociated2013,
  title = {Distance to a {{Road}} Is {{Associated}} with {{Reproductive Success}} and {{Physiological Stress Response}} in a {{Migratory Landbird}}},
  author = {Dietz, Matthew S. and Murdock, Courtney C. and Romero, L. Michael and Ozgul, Arpat and Foufopoulos, Johannes},
  year = {2013},
  month = mar,
  journal = {The Wilson Journal of Ornithology},
  volume = {125},
  number = {1},
  pages = {50--61},
  issn = {1559-4491},
  doi = {10.1676/11-201.1},
  urldate = {2023-10-17},
  abstract = {We investigated the impacts of an unpaved road on the distribution, reproduction, and stress physiology of Mountain White-crowned Sparrows (Zonotrichia leucophrys oriantha) in a high-elevation subalpine ecosystem. We found and monitored 152 sparrow nests over 3~years and captured 123 sparrows over 2~years for standardized stress series experiments. Distance of a nest to the road was associated with reproductive success, and low success near the road was primarily because of high rates of nest desertion. The probability of a nest succeeding rose with increasing distance from road up to 40~m, but nests beyond 40~m experienced lower nest success, possibly because of increased predation. Nests at intermediate distance were approximately twice as likely to succeed (70\% survival rate) as those near (36\% survival) or far (37\% survival) from the road. Stress response of male sparrows subjected to a standardized field stress protocol was elevated within 20~m of the road (38.34 {\textpm} 5.76~ng/ml) compared to those at greater distances (23.89 {\textpm} 2.01~ng/ml). Our study is the first to document the association between the distance to a road and blood corticosterone levels in any bird species. The results present strong evidence of impacts of a road on nest success of a migratory songbird. This relatively low-traffic, low-speed road clearly presents problems, even for a bird species not thought to be sensitive to disturbance.},
  file = {C\:\\Users\\nikki\\Downloads\\11-201.1.pdf;C\:\\Users\\nikki\\Zotero\\storage\\HZDFJ4ZY\\Distance-to-a-Road-is-Associated-with-Reproductive.html}
}

