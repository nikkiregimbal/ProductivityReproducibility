@article{bainesNegativeDensitydependentDispersal2020,
  title = {Negative Density-Dependent Dispersal Emerges from the Joint Evolution of Density- and Body Condition-Dependent Dispersal Strategies},
  author = {Baines, Celina B. and Travis, Justin M. J. and McCauley, Shannon J. and Bocedi, Greta},
  year = {2020},
